{
  "name": "mongo-admin",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon src/server/index.js --exec babel-node",
    "dev": "run-p -l dev:*",
    "debug:server": "DEBUG=express:* nodemon --exec babel-node -- src/server/index.js",
    "build:server": "babel src/server --out-dir dist",
    "build:client": "webpack",
    "build": "run-s -l clean build:*",
    "start": "APP_SETTINGS_URL=./settings.json NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "^6.2.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@mdi/font": "^5.3.45",
    "body-parser": "^1.18.3",
    "codemirror": "^5.55.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "json5": "^2.1.3",
    "mongodb": "^3.5.9",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "roboto-fontface": "^0.10.0",
    "simpl-schema": "^1.8.0",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.4",
    "vuex": "^3.5.1"
  }
}
