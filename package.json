    "json5": "^2.1.0",
    "mongodb": "^3.3.3",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "roboto-fontface": "^0.10.0",
    "simpl-schema": "^1.5.3",
